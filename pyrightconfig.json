{
    "venvPath": ".",
    "venv": ".venv",
    "pythonVersion": "3.11",
    "typeCheckingMode": "standard",
    "reportMissingModuleSource": false,
    "include": [
        "sotd",
        "tests"
    ],
    "exclude": [
        "**/__pycache__",
        "**/.pytest_cache",
        "**/.ruff_cache",
        ".venv",
        "venv",
        "node_modules"
    ]
}